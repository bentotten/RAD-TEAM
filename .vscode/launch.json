  "version": "0.2.0",
  "configurations": [
    {
      "name": "Build Docs",
      "type": "bashdb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/build_docs.sh",
      "args": []
    },
    {
      "name": "Eval MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/artifact_multiagent/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--mode",
        "evaluate",
        "--exp-name",
        "RAD-TEAM",
        "--obstruct",
        "0",
        "--agent-count",
        "2",
        "--net-type",
        "rnn",
        // "--seed",
        // "2",
        "--no-enforce-boundaries",
        "--no-global-critic"
        // "--render",
        // "True",
        // "--save-gif-freq",
        // "20"
      ],
      "cwd": "${workspaceFolder}/algos/artifact_multiagent"
    },
    {
      "name": "Train MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/multiagent/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--agent-count",
        "1",
        "--steps-per-epoch",
        "480",
        "--steps-per-episode",
        "120",
        "--epochs",
        "2",
        // "--render",
        // "True",
        // "--save-gif-freq",
        // "10",
        "--obstruct",
        "0",
        "--seed",
        "30176",
        // "--exp-name",
        // "TEAM-RAD",
        "--net-type",
        "cnn"
        // "--no-enforce-boundaries"
        // "--DEBUG",
        // "True",
        // "--save-freq",
        // "25",
        //"--no-global-critic",
        // "--dims",
        // "1200",
        // "1200",
        // "--area-obs",
        // "0.0",
        // "0.0"
      ],
      "cwd": "${workspaceFolder}/algos/multiagent"
    },
    {
      "name": "Test RADA2C",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/RADA2C/evaluate.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        // "--test", "FULL", 
        "--episodes", "2", 
        "--runs", "2",
        "--rada2c",
        "--render",
        "--render_freq",
        "1",
        "--obstacles",
        "1",
        // "--no-load_env",

      ],
      "cwd": "${workspaceFolder}/algos/RADA2C"
    },    
    {
      "name": "Test RADTEAM",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/RADTEAM/evaluate.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        // "--render",
        "--agents", "1", 
        "--test", "1",
        // "--epochs", "1",
        // "--steps_per_epoch", "120",
        // "--test", "0", 
        "--episodes", "2", 
        "--runs", "1"
      ],
      "cwd": "${workspaceFolder}/algos/RADTEAM"
    },
    {
      "name": "Generate Test Envs",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/utility/generate_test_envs.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        // "--test", "1"
      ],
      "cwd": "${workspaceFolder}/utility"
    },
    {
      "name": "PLOT MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/utility/plot_results.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--data_dir",
        "."
    ],
      "cwd": "${workspaceFolder}/utility/"
    },    
    {
      "name": "Bulk Evaluation test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/utility/evaluate_bulk.py",
      "console": "integratedTerminal",
      "justMyCode": false,
    //   "args": [
    //     "--data_dir",
    //     ""
    // ],
      "cwd": "${workspaceFolder}/utility/"
    },      
  ]
}
